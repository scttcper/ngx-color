{
  "name": "ngx-color",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://github.com/scttcper/ngx-color",
  "bugs": "https://github.com/scttcper/ngx-color/issues",
  "repository": "scttcper/ngx-color",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng-packagr -p ./src/lib/ng-package.json && cp README.md ./dist && cp LICENSE ./dist",
    "ghpages": "ng build --configuration production --no-progress",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeHeadlessCustom",
    "lint": "ng lint && prettier --check .",
    "lint:fix": "ng lint --fix && prettier --write .",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@ctrl/tinycolor": "4.1.0",
    "material-colors": "1.2.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.4",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/animations": "19.1.3",
    "@angular/cli": "19.1.4",
    "@angular/common": "19.1.3",
    "@angular/compiler": "19.1.3",
    "@angular/compiler-cli": "19.1.3",
    "@angular/core": "19.1.3",
    "@angular/forms": "19.1.3",
    "@angular/language-service": "19.1.3",
    "@angular/platform-browser": "19.1.3",
    "@angular/platform-browser-dynamic": "19.1.3",
    "@angular/router": "19.1.3",
    "@ctrl/ngx-github-buttons": "9.0.0",
    "@types/fs-extra": "11.0.4",
    "@types/jasmine": "5.1.5",
    "@types/node": "22.12.0",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "bootstrap": "5.3.3",
    "core-js": "3.30.2",
    "del": "8.0.0",
    "eslint": "9.19.0",
    "fs-extra": "11.3.0",
    "jasmine-core": "5.5.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "19.1.2",
    "prettier": "3.2.5",
    "puppeteer": "20.2.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "zone.js": "0.15.0"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
