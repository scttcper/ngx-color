{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "ngx-color",
  "version": "1.2.1",
  "description":
    "A Collection of Color Pickers from Sketch, Photoshop, Chrome & more",
  "dependencies": {
    "tinycolor2": "1.4.1",
    "material-colors": "1.2.5"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@angular/core": ">=5.0.0",
    "@angular/common": ">=5.0.0",
    "rxjs": "^5.5.0"
  },
  "bugs": "https://github.com/scttcper/ngx-color/issues",
  "homepage": "https://github.com/scttcper/ngx-color",
  "repository": "https://github.com/scttcper/ngx-color.git",
  "license": "MIT",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {
        "ngx-color": "ngx-color",
        "tinycolor2": "tinycolor2"
      }
    },
    "dest": "../../../dist/common"
  },
  "keywords": [
    "angular",
    "color picker",
    "angular-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ]
}
