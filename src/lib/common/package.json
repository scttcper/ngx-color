{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "ngx-color",
  "version": "0.0.0",
  "description":
    "A Collection of Color Pickers from Sketch, Photoshop, Chrome & more",
  "dependencies": {
    "tinycolor2": "^1.4.1",
    "material-colors": "^1.2.6"
  },
  "peerDependencies": {
    "@angular/core": "^5.2.0 || ^6.0.0",
    "@angular/common": "^5.2.0 || ^6.0.0",
    "rxjs": "^6.0.0"
  },
  "bugs": "https://github.com/scttcper/ngx-color/issues",
  "homepage": "https://github.com/scttcper/ngx-color",
  "repository": "https://github.com/scttcper/ngx-color.git",
  "license": "MIT",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {
        "tinycolor2": "tinycolor2",
        "ngx-color": "ngx-color"
      }
    },
    "whitelistedNonPeerDependencies": ["tinycolor2", "material-colors"],
    "dest": "../../../dist/common"
  },
  "keywords": [
    "angular",
    "color picker",
    "angular-component",
    "colorpicker",
    "picker",
    "sketch",
    "chrome",
    "photoshop",
    "material design",
    "popup"
  ]
}
